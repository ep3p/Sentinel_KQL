SecurityEvent
| where EventID == 4688
// Consider filtering on filename if perf issues occur
//| where FileName in~ ("powershell.exe","powershell_ise.exe","pwsh.exe")
| where not(ParentProcessName has_any ("gc_worker.exe", "gc_service.exe") or CommandLine has "IABbAEUAbgB2AGkAcgBvAG4AbQBlAG4AdABdADoAOgBPAFMAVgBlAHIAcwBpAG8AbgAuAFYAZQByAHMAaQBvAG4AIAA=")
| where CommandLine has "-EncodedCommand"
| parse kind=regex flags=i CommandLine with * "-EncodedCommand " EncodedCommand
| extend EncodedCommand = tostring(split(EncodedCommand, " ")[0])
| extend DecodedCommand = translate('\0', '', base64_decode_tostring(EncodedCommand))
// There might be second encoded command (cmd > powershell)
// | parse kind=regex flags=i DecodedCommand with * "-EncodedCommand " SecondEncodedCommand
// | extend SecondEncodedCommand = tostring(split(SecondEncodedCommand, " ")[0])
// | extend SecondDecodedCommand = translate('\0','', base64_decode_tostring(SecondEncodedCommand))
| extend EffectiveCommand = case(
    //isnotempty(SecondEncodedCommand), SecondDecodedCommand,
    isnotempty(EncodedCommand), DecodedCommand,
    CommandLine
    )
| where EffectiveCommand matches regex regex_empire
| project
    TimeGenerated,
    Computer,
    Account,
    AccountType,
    Activity,
    ParentProcessName,
    NewProcessName,
    CommandLine,
    EffectiveCommand,
    TargetAccount,
    SubjectLogonId,
    TargetLogonId,
    TokenElevationType
